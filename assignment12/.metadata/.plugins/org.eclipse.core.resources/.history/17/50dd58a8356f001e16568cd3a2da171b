package program04;

public class Que4 {
	static<T> int countIf(T[] arr,T key,Check<T> c) 
	{
	  	int count=0;
	  	for (T t : arr) {
	  		if(c.compare(t, key)) 
	  			count++;
	  	}
			
		return count;
	}

	public static void main(String[] args) {
		Double [] arr= {44,77,99,22,55,66};
		Integer key=50;
		
		int cnt=countIf(arr, key,(x,y)->x>y);
		System.out.println("Result :"+cnt);
	}

}
