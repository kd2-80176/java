package program01que1;

public class Demo01GenericMethod {

	static <T extends Number> T findMin(T[] arr) {
		T min;
		for (int i = 0; i < 5; i++) {
			for(int j=0;j<5;j++)
			{
				if(arr[i]<arr[j+1])
					min=arr[i];
			}
      
		}
	}

	public static void main(String[] args) {
		Integer[] arr1 = { 22, 55, 77, 11, 33 };
		findMin(arr1);
	}

}
